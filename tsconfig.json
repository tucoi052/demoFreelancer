{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "target": "esnext",
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext",
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "Components/*": [
        "/components/*"
      ],
      "Screens/*": [
        "/screens/*"
      ],
      "Utils/*": [
        "/utils/*"
      ]
    }
  },
  "exclude": [
    "__tests__/**/*-test.ts",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}